---
import Layout from '../layouts/Layout.astro';
import Navigation from '../components/Navigation.astro';
import Hero from '../components/Hero.astro';
import Benefits from '../components/Benefits.astro';
import Process from '../components/Process.astro';
import TechLogos from '../components/TechLogos.astro';
import ResultsPreview from '../components/ResultsPreview.astro';
import Assessment from '../components/Assessment.astro';
import Contact from '../components/Contact.astro';
import Footer from '../components/Footer.astro';
---

<Layout 
  title="amotivv - AI consulting"
  description="Expert AI consulting to help businesses navigate artificial intelligence adoption. Get personalized assessments, implementation roadmaps, and continuous support for your AI journey."
  type="website"
  keywords={[
    "AI consulting",
    "artificial intelligence consulting",
    "AI implementation",
    "business AI solutions",
    "AI strategy",
    "technology consulting",
    "digital transformation",
    "AI assessment",
    "AI roadmap",
    "enterprise AI"
  ]}
>
  <Navigation />
  <main>
    <Hero />
    <Benefits />
    <TechLogos />
    <Process />
    <ResultsPreview />
    <Assessment />
    <Contact />
  </main>
  <Footer />
</Layout>
